{
  "compilerOptions": {
    "noImplicitUseStrict": true,
    "alwaysStrict": false,
    "skipLibCheck": true,
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.cache/.tsbuildinfo",
    "outDir": "out",
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "noEmit": true,
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    "public",
    ".cache"
  ]
}
